[alias]
    c = commit -am
    up = pull --prune
    p = push
    s = status -s
    df = diff --color --color-words --abbrev
    co = checkout
    cob = checkout -b
    cm = !git add -A && git commit -m

[credential]
	helper =
	helper = store --file /opt/dev/var/private/git_credential_store
[core]
	commitGraph = true
	mergeoptions = --no-edit
	excludesfile = /Users/michaelwilson/.gitignore

[pull]
	rebase = false

[push]
	default = current

[diff]
	algorithm = patience
[gc]
	writeCommitGraph = true
[protocol]
	version = 2
[url "https://github.com/Shopify/"]
	insteadOf = git@github.com:Shopify/
	insteadOf = git@github.com:shopify/
	insteadOf = ssh://git@github.com/Shopify/
	insteadOf = ssh://git@github.com/shopify/
[user]
	name = Mike Wilson
	email = michael.wilson@shopify.com
	signingkey = AE2455ED563384B7458A49C97B7494C29DCC73D2
[gpg]
	program = /nix/var/nix/gcroots/dev-profiles/dev-support-dev-profile/bin/gpg-auto-pin
[commit]
	gpgSign = true
[init]
	defaultBranch = main
